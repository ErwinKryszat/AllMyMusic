<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:AllMyMusic_v3.ViewModel"
                    xmlns:vw="clr-namespace:AllMyMusic_v3.View">

    <DataTemplate DataType="{x:Type vm:RibbonViewModel}" >
        <vw:viewRibbon />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ToolbarViewModel}" >
        <vw:viewToolbarTray />
    </DataTemplate>  

    <DataTemplate DataType="{x:Type vm:BandListViewModel}" >
        <vw:viewBandPanel />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AlbumAndAlbumListViewModel}" >
        <vw:viewAlbumListAndAlbum />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AlbumListViewModel}" >
        <vw:viewAlbumList />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AlbumViewModel}" >
        <vw:viewAlbum />
    </DataTemplate>

    
    <DataTemplate DataType="{x:Type vm:AlbumGenreViewModel}" >
        <vw:viewAlbumGenrePanel />
    </DataTemplate>


    <DataTemplate DataType="{x:Type vm:TaskQueueViewModel}" >
        <vw:viewStatusBar />
    </DataTemplate>

</ResourceDictionary>